using System;
class Program14
{
    static void Main()
    {
        Console.Write("Enter the number of terms: ");
        int n = int.Parse(Console.ReadLine());
        int a = 0, b = 1;
        Console.Write($"Fibonacci Series: {a} {b} ");
        for (int i = 2; i < n; i++)
        {
            int temp = a + b;
            Console.Write($"{temp} ");
            a = b;
            b = temp;
        }
        Console.WriteLine();
    }
}
