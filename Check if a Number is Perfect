using System;
class Program28
{
    static void Main()
    {
        Console.Write("Enter a number: ");
        int num = int.Parse(Console.ReadLine());
        int sum = 0;

        for (int i = 1; i < num; i++)
        {
            if (num % i == 0)
            {
                sum += i;
            }
        }

        Console.WriteLine(num == sum ? $"{num} is a Perfect number." : $"{num} is not a Perfect number.");
    }
}
